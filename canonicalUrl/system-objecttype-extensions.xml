<type-extension type-id="SitePreferences">
    <attribute-definition attribute-id="excludedCanonicalCategories">
        <display-name xml:lang="x-default">Excluded canonical category ids (comma-separated)</display-name>
        <type>text</type>
        <mandatory-flag>false</mandatory-flag>
        <externally-managed-flag>false</externally-managed-flag>
    </attribute-definition>
    <attribute-definition attribute-id="materialSelfReferencingCanonicalCategories">
        <display-name xml:lang="x-default">Material self-referencing canonical category ids (comma-separated)</display-name>
        <type>text</type>
        <mandatory-flag>false</mandatory-flag>
        <externally-managed-flag>false</externally-managed-flag>
    </attribute-definition>
    <attribute-definition attribute-id="styleSelfReferencingCanonicalCategories">
        <display-name xml:lang="x-default">Style self-referencing canonical category ids (comma-separated)</display-name>
        <type>text</type>
        <mandatory-flag>false</mandatory-flag>
        <externally-managed-flag>false</externally-managed-flag>
    </attribute-definition>
    <attribute-definition attribute-id="typeSelfReferencingCanonicalCategories">
        <display-name xml:lang="x-default">Type self-referencing canonical category ids (comma-separated)</display-name>
        <type>text</type>
        <mandatory-flag>false</mandatory-flag>
        <externally-managed-flag>false</externally-managed-flag>
    </attribute-definition>
    <attribute-definition attribute-id="colorSelfReferencingCanonicalCategories">
        <display-name xml:lang="x-default">Color self-referencing canonical category ids (comma-separated)</display-name>
        <type>text</type>
        <mandatory-flag>false</mandatory-flag>
        <externally-managed-flag>false</externally-managed-flag>
    </attribute-definition>
    <group-definitions>
        <attribute-group group-id="juicyBox">
            <display-name xml:lang="x-default">Juicy Box</display-name>
            <attribute attribute-id="excludedCanonicalCategories"/>
            <attribute attribute-id="materialSelfReferencingCanonicalCategories"/>
            <attribute attribute-id="styleSelfReferencingCanonicalCategories"/>
            <attribute attribute-id="typeSelfReferencingCanonicalCategories"/>
            <attribute attribute-id="colorSelfReferencingCanonicalCategories"/>
        </attribute-group>
    </group-definitions>
</type-extension>
